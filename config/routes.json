{
  "routes": [
    {
      "method": "GET",
      "path": "/reload",
      "handler": "meilisearch.reload",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/credentials",
      "handler": "meilisearch.getClientCredentials",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "POST",
      "path": "/credentials",
      "handler": "meilisearch.addCredentials",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/collections",
      "handler": "meilisearch.getCollections",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "POST",
      "path": "/collections/:collection",
      "handler": "meilisearch.addCollection",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "PUT",
      "path": "/collections/:collection",
      "handler": "meilisearch.updateCollections",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/collections/:collection",
      "handler": "meilisearch.removeCollection",
      "config": {
        "policies": ["isAdmin"]
      }
    }
  ]
}
